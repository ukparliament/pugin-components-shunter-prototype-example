{
  "name": "pugin-components-shunter-prototype-example",
  "version": "1.0.0",
  "description": "A simple example prototype running on shunter with pugin-components and Heroku support",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node app.js -s --compile-on-demand",
